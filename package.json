  },
  "bugs": {
    "email": "1801982702@qq.com",
    "url": "https://github.com/markthree/n-init/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.17.5",
    "bumpp": "^9.1.1",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1"
  }
}
